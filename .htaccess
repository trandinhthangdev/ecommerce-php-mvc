RewriteEngine On


# Client

RewriteRule ^(.+).html/([0-9]+)-.+/([0-9]+)-.+$			index.php?action=$1&l1=$2&l2=$3 [L,QSA]

RewriteRule ^(.+).html/([0-9]+)-.+$			index.php?action=$1&l1=$2 [L,QSA]
RewriteRule ^(.+).html/([0-9]+)$			index.php?action=$1&l1=$2 [L,QSA]
RewriteRule ^(.+).html/(.+)$			index.php?action=$1&key_word=$2 [L,QSA]

#RewriteRule ^(.+).html/(.+)$			index.php?action=$1&l1=$2  [L,QSA]
RewriteRule ^(.+).html$			index.php?action=$1


